<div class="container">
    <div class="row content">
        <div class="col-md-6">
            <img src="img/login-page.svg" alt="image" srcset="" class="img-fluid loginImg">
        </div>
        <div class="col-md-6">
            <h3 class="signin-text mb-3">Sign In</h3>
            <form action="" class="form">
                <div class="mb-3">
                    <label for="email">Email</label>
                    <input class="form-control" type="email" name="email" id="email">
                </div>
                <div class="mb-3">
                    <label for="password">Password</label>
                    <input class="form-control" type="password" name="password" id="password">
                </div>
                <div class="mb-3 form-check">
                    <input type="checkbox" name="checkbox" id="" class ="form-check-input">
                    <label for="checkbox">Remember Me</label>
                </div>
                <div class ="d-grid gap-2  mx-auto">
                    <button class="btn btn-class" type="submit" name="submit">Login</button>
                </div>

                <div class="mt-3" >
                    <a class = "btn-link" href="/register">Register</a>
                </div>
                <div class="mt-3" >
                    <a class = "btn-link" href="/dashboard">Login w/d creds</a>
                </div>
                
            </form>
        </div>
    </div>
</div>